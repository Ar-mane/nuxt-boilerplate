{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "NITRO_PRESET=vercel nuxt build",
    "build:dev": "NITRO_PRESET=node-server nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:fix": "yarn lint:ts:fix && yarn lint:scss:fix",
    "lint:ts": "eslint --ext \".ts,.js,.vue\" .",
    "lint:scss": "stylelint \"**/*.{css,scss,vue}\" && prettier --check \"**/*.{css,scss}\"",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:scss:fix": "stylelint --fix \"**/*.{css,scss,vue}\" && prettier -w \"**/*.{css,scss}\"",
    "lint:staged:fix": "lint-staged --concurrent false",
    "postinstall": "nuxt prepare && husky install",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@nuxt/image": "^1.4.0",
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@vercel/speed-insights": "^1.0.10",
    "@vueuse/core": "^10.9.0",
    "clsx": "^2.1.0",
    "normalizecss": "^3.0.0",
    "nuxt-seo-kit": "^1.3.7",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@kevinmarrec/nuxt-pwa": "^0.17.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "eslint-plugin-vue-scoped-css": "^2.8.0",
    "husky": "^8.0.3",
    "js-to-scss": "^1.0.0",
    "lint-staged": "^13.1.0",
    "nuxt": "^3.10.1",
    "nuxt-purgecss": "^2.0.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.57.1",
    "stylelint": "^16.2.0",
    "stylelint-config-clean-order": "^5.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite-plugin-compression2": "^1.0.0",
    "vue-tsc": "^1.8.27"
  }
}
