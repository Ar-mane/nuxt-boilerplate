{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "NITRO_PRESET=vercel nuxt build",
    "build:dev": "NITRO_PRESET=node-server nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:fix": "yarn lint:ts:fix && yarn lint:scss:fix",
    "lint:ts": "eslint --ext \".ts,.js,.vue\" .",
    "lint:scss": "stylelint \"**/*.{css,scss,vue}\" && prettier --check \"**/*.{css,scss}\"",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:scss:fix": "stylelint --fix \"**/*.{css,scss,vue}\" && prettier -w \"**/*.{css,scss}\"",
    "lint:staged:fix": "lint-staged --concurrent false",
    "postinstall": "nuxt prepare && cd .. && husky install front/.husky",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/robots": "^4.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@vercel/speed-insights": "^1.0.12",
    "@volar/typescript": "^2.3.4",
    "@vueuse/core": "^10.11.0",
    "clsx": "^2.1.1",
    "normalizecss": "^3.0.0",
    "nuxt-seo-kit": "^1.3.13",
    "pinia": "^2.1.7",
    "qs": "^6.12.3",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@kevinmarrec/nuxt-pwa": "^0.17.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vue-scoped-css": "^2.8.1",
    "husky": "^9.0.11",
    "js-to-scss": "^1.1.0",
    "lint-staged": "^15.2.7",
    "nuxt": "^3.12.3",
    "nuxt-purgecss": "^2.0.0",
    "postcss": "^8.4.39",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.77.8",
    "stylelint": "^16.7.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.1",
    "tailwindcss": "^3.4.5",
    "typescript": "^5.5.3",
    "vite-plugin-compression2": "^1.1.2",
    "vue-tsc": "^2.0.26"
  }
}
